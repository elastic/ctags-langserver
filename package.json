{
	"name": "@elastic/ctags-langserver",
    "version": "1.0.0",
    "description": "An all-languages language server built on top of ctags",
    "author": "Elastic",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/elastic/ctags-langserver.git"
    },
    "files": [
        "lib"
	],
	"main": "./lib/cli.js",
    "bin": {
        "ctags-language-server": "./lib/cli.js"
    },
	"binary": {
		"module_name": "ctags",
		"module_path": "./build/Release/",
		"host": "https://download.elasticsearch.org/code/ctags/"
	},
	"scripts": {
		"compile": "tsc -b",
		"build": "node-gyp configure build",
		"tslint": "tslint -c tslint.json -p .",
		"test": "jest",
		"prepare": "echo 'skip'",
        "clean": "rimraf lib && node-pre-gyp clean",
		"lint": "tslint -c ../tslint.json --project .",
		"install": "node-pre-gyp install --fallback-to-build",
		"prepublish": "node-pre-gyp package publish info",
		"unpublish": "node-pre-gyp unpublish"
	},
	"dependencies": {
		"commander": "^2.11.0",
		"vscode-languageserver": "^5.2.1",
		"nuclide-prebuilt-libs": "^0.5.7",
		"line-column": "^1.0.2",
		"@elastic/lsp-extension": "^0.1.1",
		"grep1": "^0.0.5",
		"node-gyp": "^4.0.0",
		"node-pre-gyp": "^0.13.0"
    },
	"devDependencies": {
		"@types/commander": "^2.9.2",
		"@types/node": "^12.0.4",
		"@types/jest": "24.0.13",
		"tslint": "^5.16.0",
		"typescript": "^3.4.5",
		"jest": "^24.8.0",
		"ts-jest": "^24.0.2",
		"ts-node": "7.0.1",
		"aws-sdk": "^2.477.0"
	}
}
